<h1 class="text-center">Your articles</h1>
<% if @cart.cart_elements.present? %>
  <div class="container-wrap" id="cart-elements-container">
    <%= render "cart_elements/render_cart_elements", cart: @cart %>
  </div>
<% else %>
  <div class="text-center">You don't have any article yet</div>
<% end %>